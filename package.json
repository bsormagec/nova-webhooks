{
    "private": true,
    "scripts": {
        "dev": "NODE_ENV=development mix --mix-config=./webpack.mix.js",
        "development": "NODE_ENV=development mix --mix-config=./webpack.mix.js",
        "watch": "NODE_ENV=development mix --mix-config=./webpack.mix.js watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000 --mix-config=./webpack.mix.js",
