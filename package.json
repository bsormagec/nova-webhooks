        "prod": "npm run production",
        "production": "NODE_ENV=production mix --mix-config=./webpack.mix.js --production"
    },
    "devDependencies": {
        "cross-env": "^7.0.0",
        "laravel-mix": "^6.0",
        "vue-loader": "^15.9.7",
